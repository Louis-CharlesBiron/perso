{
  "name": "Time Since...",
  "description": "Lets you easily and precisely track how much time has pass since you started doing something or since something happened!",
  "version": "1.0.4",
  "manifest_version": 3,
  "icons": {
      "48": "./img/icon.png"
      
  },
    "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+L",
        "linux": "Ctrl+Shift+L",
        "windows": "Ctrl+Shift+L",
        "mac": "Alt+Shift+L"
      }
    }
  },
  "background": {
      "service_worker": "background.js"
  },
  "action": {
    "default_title": "Open...",
      "default_popup": "init/sc.html"
  },
  "permissions": [
    "storage",
    "unlimitedStorage",
    "tabs"
]
}

