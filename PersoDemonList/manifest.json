{
  "name": "My DemonList",
  "description": "Create your own demonlist!",
  "version": "1.1",
  "version_name": "1.1",
  "manifest_version": 3,
  "icons": {
      "128": "./img/icon-128x128.png"
  },
  "commands": {
    "_execute_action": { 
      "suggested_key": {
        "default": "Ctrl+Shift+H",
        "linux": "Ctrl+Shift+H",
        "windows": "Ctrl+Shift+H",
        "mac": "Alt+Shift+H"
      }
    }
  },
  "background": {
      "service_worker": "background.js"
  },
  "action": {
    "default_title": "My Demonlist",
      "default_popup": "popup.html"
  },
  "permissions": [
    "storage"
  ]
}

