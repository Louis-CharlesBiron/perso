{
  "name": "Geometry Dash DemonList Extension",
  "short_name": "GD DL",
  "description": "Better DemonList",
  "version": "1",
  "version_name": "(Perso)",
  "manifest_version": 3,
  "icons": {
      "128": "./img/icon.png"
      
  },
  "background": {
      "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.pointercrate.com/demonlist/*"],
      "js": ["./backdrop/content.js"]
    }
  ],
  "action": {
    "default_title": "Browser_Action title",
      "default_popup": "popup.html",
      "default_icons": {
          "16": "./img/icon-16x16.png",
          "32": "./img/icon-32x32.png",
          "48": "./img/icon-48x48.png",
          "128": "./img/icon-128x128.png"
      }
  },
  "permissions": [
    "storage"
]
}

