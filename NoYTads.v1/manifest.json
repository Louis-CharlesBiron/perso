{
  "name": "Instant Youtube Ads",
  "description": "Automatically skips youtube ads",
  "version": "1.2",
  "version_name": "1.2",
  "manifest_version": 3,
  "icons": {
      "128": "./img/icon.png"
  },
  "background": {
      "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["./backdrop/content.js"]
    }
  ],
  "permissions": [
    "tabs"
  ]
}

