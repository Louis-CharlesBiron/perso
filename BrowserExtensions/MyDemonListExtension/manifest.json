{
    "name": "My DemonList",
    "description": "Create your own demonlist!",
    "version": "2.004",
    "version_name": "2.004",
    "manifest_version": 3,
    "icons": {
        "128": "/assets/favicon.png"
    },
    "commands": {
      "_execute_action": { 
        "suggested_key": {
          "default": "Ctrl+Shift+H",
          "linux": "Ctrl+Shift+H",
          "windows": "Ctrl+Shift+H",
          "mac": "Alt+Shift+H"
        }
      }
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
      "default_title": "My Demonlist",
        "default_popup": "index.html"
    },
    "permissions": [
      "storage",
      "unlimitedStorage",
      "downloads",
      "declarativeNetRequestWithHostAccess"
    ],
    "host_permissions": [
      "https://www.youtube.com/*"
    ]
  }
  
  